{
  "name": "risu-handdam-edit",
  "version": "0.8.0",
  "description": "RisuAI 한땀한땀 수정 지원 Plugin",
  "main": "src/index.js",
  "browser": "dist/risu-handdam-edit.js",
  "unpkg": "dist/risu-handdam-edit.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "risu-ai",
    "cdn",
    "plugin"
  ],
  "author": "Chaebboong",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently \"caddy run --config caddy.config --adapter caddyfile\" \"npm run dev:server\" \"npm run dev:webpack\"",
    "dev:caddy": "caddy run --config caddy.config --adapter caddyfile",
    "dev:server": "node scripts/dev-server.js",
    "dev:webpack": "cross-env NODE_ENV=development webpack --mode production --watch",
    "build": "webpack --mode production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "release": "node scripts/release.js",
    "release:minor": "node scripts/release.js -- minor",
    "release:patch": "node scripts/release.js -- patch",
    "release:major": "node scripts/release.js -- major"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "ws": "^8.18.3"
  },
  "dependencies": {
    "idb": "^8.0.0",
    "winbox": "^0.2.82"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
